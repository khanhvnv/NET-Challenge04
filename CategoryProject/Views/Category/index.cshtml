@model List<CategoryProject.Models.Category>

@{
    ViewData["Title"] = "Categories";
}

<h1>Categories</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.ID</td>
                <td>@category.Name</td>
            </tr>
        }
    </tbody>
</table>
